local function convertMessage(message)
    local converted = ""
    for char in message:gmatch(".") do
        converted = converted .. (charMap[char] or char)
    end
    return converted
end

local Toggle = Tab:CreateToggle({
    Name = "Auto Chat Bypass",
    CurrentValue = false,
    Flag = "Toggle1",
    Callback = function(Value)
        isToggleOn = Value
        Rayfield:Notify({
            Title = "‚≠êStarBypass‚≠ê",
            Content = isToggleOn and "Auto Chat Bypass Enabled!" or "Auto Chat Bypass Disabled!",
            Duration = 5
        })
    end,
})

local player = game.Players.LocalPlayer
local DefaultChatSystem = game.ReplicatedStorage:WaitForChild("DefaultChatSystemChatEvents", 10)
local SayMessageRequest = DefaultChatSystem and DefaultChatSystem:WaitForChild("SayMessageRequest", 10)

if SayMessageRequest then
    player.Chatted:Connect(function(message)
        if isToggleOn then
            local convertedMessage = convertMessage(message)
            SayMessageRequest:FireServer(convertedMessage, "All")
        end
    end)
end

local TextChatService = game:GetService("TextChatService")
if TextChatService then
    local function onLocalMessage(message)
        if isToggleOn and message.TextSource == player then
            local convertedMessage = convertMessage(message.Text)
            SayMessageRequest:FireServer(convertedMessage, "All")
            return nil
        end
        return message
    end
    TextChatService.OnIncomingMessage = onLocalMessage
end

local Input = Tab:CreateInput({
    Name = "Manual Chat Bypass",
    CurrentValue = "",
    PlaceholderText = "Input Text",
    RemoveTextAfterFocusLost = false,
    Flag = "Input1",
    Callback = function(Text)
        if Text and Text ~= "" then
            local convertedMessage = convertMessage(Text)
            SayMessageRequest:FireServer(convertedMessage, "All")
        end
    end,
})

